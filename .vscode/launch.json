  "version": "0.2.0",
  "configurations": [
    {
      "name": "Gateway",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/api-gateway/main.go",
      "cwd": "${workspaceFolder}/api-gateway",
      "env": {
        "SERAPH_AUTH_ENABLED": "false"
      }
    },
    {
      "name": "file-provider-dir",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/file-provider-dir/main.go"
    },
    {
      "name": "file-provider-smb",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/file-provider-smb/main.go",
      "console": "integratedTerminal"
    },
    {
      "name": "file-indexer",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/file-indexer/main.go"
    },
    {
      "name": "shares",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/shares/main.go"
